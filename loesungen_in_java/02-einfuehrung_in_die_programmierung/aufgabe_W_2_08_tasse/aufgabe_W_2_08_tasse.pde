// Setze Fenstergröße auf 600x600 Pixel
size(600, 600);

// Setze Hintergrundfarbe
background(116, 168, 146);

// Zeichnungsparameter für Tasse setzen
strokeWeight(2);
stroke(125);
fill(251, 242, 196);

// Tassenkörper
rect(100, 100, 300, 350);

// Tassenhenkel
rect(400, 150, 150, 200);

// Zeichnungsparameter für Tassenhenkellücke setzen
strokeWeight(2);
stroke(125);
fill(116, 168, 146);

// Tassenhenkellücke
rect(450, 200, 50, 100);

// Zeichnungsparameter für Beschriftung setzen
strokeWeight(2);
stroke(199, 82, 42);
fill(199, 82, 42);

// Tassenbeschriftung
rect(150, 150, 150, 50);
rect(200, 200, 50, 150);
