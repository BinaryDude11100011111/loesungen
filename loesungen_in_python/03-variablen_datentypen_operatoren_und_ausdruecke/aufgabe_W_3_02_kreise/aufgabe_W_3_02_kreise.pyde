size(550, 150)

# x-Koordinate des Kreismittelpunkts des ersten Kreises
x = 75

# y-Koordinate des Kreismittelpunkts
y = 70

# Radius des Kreises
r = 100

# Zeichne vier Kreise mit Variablen und Operatoren
ellipse(x, y, r, r)
ellipse(x + r, y, r, r)
ellipse(x + r * 2, y, r, r)
ellipse(x + r * 3, y, r, r)
ellipse(x + r * 4, y, r, r)

